    {
        "versioninfo": "Release 3.11.1.0, version 2021102700 on Moodle 3.11.4+ (Build: 20220111)",
        "maturity": "Maturity info",
        "maturityalert": "info",
        "versioncheck": "Not supported on this version of Moodle"
